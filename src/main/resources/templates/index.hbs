
{{#partial "content"}}

    <h1> Heroes</h1>

    {{#if heroes}}
        {{#each heroes }}
            <p>current hero</p>
            <p>{{ name }} <a href="/squads/{{squadId}}/heroes/{{id}}">{{name}}</a></p>
        {{/each}}
    {{else}}
        <p>No Heroes!</p>
    {{/if}}
{{/partial}}

{{> layout.hbs}}
